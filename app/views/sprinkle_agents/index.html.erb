<p id="notice"><%= notice %></p>

<h1>Sprinkle Agents</h1>

<table>
  <thead>
    <tr>
      <th>Sprinkle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sprinkle_agents.each do |sprinkle_agent| %>
      <tr>
        <td><%= sprinkle_agent.sprinkle_id %></td>
        <td><%= link_to 'Show', sprinkle_agent %></td>
        <td><%= link_to 'Edit', edit_sprinkle_agent_path(sprinkle_agent) %></td>
        <td><%= link_to 'Destroy', sprinkle_agent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sprinkle Agent', new_sprinkle_agent_path %>
